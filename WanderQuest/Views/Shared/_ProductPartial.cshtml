@model List<Product>

@for (int i = 0; i < Model.Count; i++)
{
    <div class="package-card" data-category="@Model[i].Category.Name.ToLower()">
        <img src="@Model[i].ImgUrl" alt="$@Model[i].Title">
        <div class="package-card-content">
            <h3>@Model[i].Title</h3>
            <p>@Model[i].Description</p>
            <div class="price">"$@Model[i].Price"</div>
            <button data-id="@Model[i].Category.Id">Add to Wishlist</button>
        </div>
    </div>
}